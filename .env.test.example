# Twilio Configuration for Testing
# Get these from your Twilio Console: https://console.twilio.com/

# Twilio Account Credentials
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here

# Twilio Phone Number (must be a verified Twilio number)
TWILIO_PHONE_NUMBER=+1234567890

# LiveKit Configuration (should match your existing .env)
LIVEKIT_URL=wss://your-livekit-instance.livekit.cloud
LIVEKIT_API_KEY=your_livekit_api_key_here
LIVEKIT_API_SECRET=your_livekit_api_secret_here

# Render Deployment URL (where your voice agent is running)
RENDER_WEBHOOK_URL=https://your-app.onrender.com

# Optional: Webhook URL for call status updates
TWILIO_STATUS_WEBHOOK=https://your-app.onrender.com/webhook/twilio/status

# Groq API Key (for STT and LLM services)
GROQ_API_KEY=your_groq_api_key_here
